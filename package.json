    "es6-promisify": "^5.0.0",
    "express": "^4.17.1",
    "express-winston": "^2.4.0",
    "fbjs": "^0.8.16",
    "http-status": "^1.4.2",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
